package model;

public class Drugstore {
	
	private String name;
	private String loc;

	// 기본생성자
	public Drugstore() {
	}

	//getter, setter name
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	
	public Drugstore(String name, String loc) {
		this.name = name;
		this.loc = loc;
	}
	//getter, setter loc
	public String getLoc() {
		return loc;
	}
	
	public void setLoc(String loc) {
		this.loc = loc;
	}
	
}
